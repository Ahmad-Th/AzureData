{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azdf-main-athahabee"
		},
		"AZ_LS_CSVFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AZ_LS_CSVFiles'"
		},
		"azls_sql_Cameras_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azls_sql_Cameras'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azdf-main-athahabee_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AZ_LS_CSVFiles_connectionString": {
						"value": "[parameters('AZ_LS_CSVFiles_connectionString')]"
					},
					"azls_sql_Cameras_connectionString": {
						"value": "[parameters('azls_sql_Cameras_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}