{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azdf-main-athahabee"
		},
		"AZ_LS_CSVFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AZ_LS_CSVFiles'"
		},
		"azls_sql_Cameras_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azls_sql_Cameras'"
		},
		"AZKV_LinkedService_SQLDB_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azkv-main-athahabee.vault.azure.net/"
		},
		"AZLS_Web_MoviesCSV_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/aws-samples/aws-ml-data-lake-workshop/master/dms/data/movies.csv"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AZKV_LinkedService_SQLDB')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('AZKV_LinkedService_SQLDB_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AZ_LS_CSVFiles')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Link to main CSV source file",
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('AZ_LS_CSVFiles_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/azls_sql_Cameras')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('azls_sql_Cameras_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AZIR-Main')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/DSCameraList')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AZ_LS_CSVFiles",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "FMS - Cameras.csv",
						"container": "csvs"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Status",
						"type": "String"
					},
					{
						"name": "Serial",
						"type": "String"
					},
					{
						"name": "Asset",
						"type": "String"
					},
					{
						"name": "SIM Number",
						"type": "String"
					},
					{
						"name": "Last Communication",
						"type": "String"
					},
					{
						"name": "Last Pending Update",
						"type": "String"
					},
					{
						"name": "Update Manager",
						"type": "String"
					},
					{
						"name": "Vision",
						"type": "String"
					},
					{
						"name": "Telematics",
						"type": "String"
					},
					{
						"name": "Firmware",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AZ_LS_CSVFiles')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/azds_sql_Cameras')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "azls_sql_Cameras",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "Common",
					"table": "Camera"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/azls_sql_Cameras')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AZLS_Web_MoviesCSV')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Movies table CSV source",
				"annotations": [],
				"type": "Web",
				"typeProperties": {
					"url": "[parameters('AZLS_Web_MoviesCSV_properties_typeProperties_url')]",
					"authenticationType": "Anonymous"
				},
				"connectVia": {
					"referenceName": "AZIR-Main",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AZIR-Main')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/MoviesWebTable')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AZLS_Web_MoviesCSV",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "WebTable",
				"schema": [],
				"typeProperties": {
					"index": 0
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AZLS_Web_MoviesCSV')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AZDF_CopyCameras')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "DSCameraList",
								"type": "DatasetReference"
							},
							"name": "source1"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "azds_sql_Cameras",
								"type": "DatasetReference"
							},
							"name": "sink1"
						}
					],
					"transformations": [
						{
							"name": "select1"
						}
					],
					"scriptLines": [
						"source(output(",
						"          Status as integer,",
						"          Serial as integer,",
						"          Asset as string,",
						"          {SIM Number} as string,",
						"          {Last Communication} as date,",
						"          {Last Pending Update} as date,",
						"          {Update Manager} as string,",
						"          Vision as string,",
						"          Telematics as string,",
						"          Firmware as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false,",
						"     preferredIntegralType: 'integer',",
						"     preferredFractionalType: 'decimal') ~> source1",
						"source1 select(mapColumn(",
						"          Status,",
						"          Serial,",
						"          Asset,",
						"          {SIM Number},",
						"          {Last Communication},",
						"          {Last Pending Update},",
						"          {Update Manager},",
						"          Vision,",
						"          Telematics,",
						"          Firmware",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> select1",
						"select1 sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DSCameraList')]",
				"[concat(variables('factoryId'), '/datasets/azds_sql_Cameras')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AZPIP_CopyCameras')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Copy cameras from blob to sql db",
				"activities": [
					{
						"name": "AZDF_CopyCameras",
						"type": "ExecuteDataFlow",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "AZDF_CopyCameras",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"source1": {},
									"sink1": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/dataflows/AZDF_CopyCameras')]"
			]
		}
	]
}