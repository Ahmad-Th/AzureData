{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DSCameraList",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Status as string,",
				"          Serial as string,",
				"          Asset as string,",
				"          {SIM Number} as string,",
				"          {Last Communication} as string,",
				"          {Last Pending Update} as string,",
				"          {Update Manager} as string,",
				"          Vision as string,",
				"          Telematics as string,",
				"          Firmware as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          Status,",
				"          Serial,",
				"          Asset,",
				"          {SIM Number},",
				"          {Last Communication Date} = {Last Communication},",
				"          {Last Pending Update Date} = {Last Pending Update},",
				"          {Update Manager Ver} = {Update Manager},",
				"          Vision,",
				"          Telematics,",
				"          Firmware",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(",
				"toDate({Last Communication Date}, \"yyyy-MM-dd HH:mm:ss\") < currentDate()) ~> filter1",
				"filter1 alterRow() ~> alterRow1"
			]
		}
	}
}