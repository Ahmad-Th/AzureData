{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DSCameraList",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Status as string,",
				"          Serial as string,",
				"          Asset as string,",
				"          {SIM Number} as string,",
				"          {Last Communication} as string,",
				"          {Last Pending Update} as string,",
				"          {Update Manager} as string,",
				"          Vision as string,",
				"          Telematics as string,",
				"          Firmware as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     preferredIntegralType: 'integer',",
				"     preferredFractionalType: 'decimal') ~> source1"
			]
		}
	}
}